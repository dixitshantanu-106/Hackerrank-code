    "from math import sqrt\n",
    "def squares(a, b):\n",
    "    sq = 0\n",
    "    for i in range(int(sqrt(a)), int(sqrt(b))+1):\n",
    "        if i**2 >= a and i**2 <= b:\n",
    "            sq += 1\n",
    "    return sq"
   ]
  }
